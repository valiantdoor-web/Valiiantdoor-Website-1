{\n  "version": 2,\n  "builds": [\n    {\n      "src": "api/index.js",\n      "use": "@vercel/node"\n    }\n  ],\n  "routes": [\n    {\n      "src": "/api/(.*)",\n      "dest": "/api/index.js"\n    },\n    {\n      "src": "/",\n      "dest": "/index.html"\n    },\n    {\n      "src": "/(.*\.(css|js|json|xml|txt|png|jpg|jpeg|gif|svg|webp|ico|woff|woff2|ttf|eot|html))",\n      "dest": "/$1"\n    },\n    {\n      "src": "/(.*)",\n      "dest": "/api/index.js"\n    }\n  ]\n}